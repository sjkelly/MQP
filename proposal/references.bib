@article{Newman_Yi_2006, title={A survey of the marching cubes algorithm}, volume={30}, ISSN={0097-8493}, url={http://www.sciencedirect.com/science/article/pii/S0097849306001336}, DOI={10.1016/j.cag.2006.07.021}, abstractNote={A survey of the development of the marching cubes algorithm [W. Lorensen, H. Cline, Marching cubes: a high resolution 3D surface construction algorithm. Computer Graphics 1987; 21(4):163–9], a well-known cell-by-cell method for extraction of isosurfaces from scalar volumetric data sets, is presented. The paper’s primary aim is to survey the development of the algorithm and its computational properties, extensions, and limitations (including the attempts to resolve its limitations). A rich body of publications related to this aim are included. Representative applications and spin-off work are also considered and related techniques are briefly discussed.}, number={5}, journal={Computers \& Graphics}, author={Newman, Timothy S. and Yi, Hong}, year={2006}, month={Oct}, pages={854–879}}

